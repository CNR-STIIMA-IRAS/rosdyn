# - Config file for the rdyn_core package
# It defines the following variables
#  rdyn_core_INCLUDE_DIRS - include directories for FooBar
#  rdyn_core_LIBRARIES    - libraries to link against
#  rdyn_core_EXECUTABLE   - the bar executable

set(rdyn_core_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
include("${rdyn_core_DIR}/export_rdyn_core.cmake")

include(CMakeFindDependencyMacro)
find_dependency(eigen_matrix_utils REQUIRED)
find_dependency(kinematics_filters REQUIRED)


# These are IMPORTED targets created by rdyn_coreTargets.cmake
set(rdyn_core_LIBRARIES rdyn_core_utilities)

set(rdyn_core_EXECUTABLE rdyn_core_server)

